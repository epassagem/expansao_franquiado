<h1>Listing interessados</h1>

<%= will_paginate @interessados %>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Rg</th>
      <th>Data Nascimento</th>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @interessados.each do |interessado| %>
      <tr>
        <td><%= interessado.nome %></td>
        <td><%= interessado.cpf %></td>
        <td><%= interessado.rg %></td>
        <td><%= interessado.data_nasc %></td>
        <td><%= interessado.email %></td>
        <td><%= link_to 'Exibir', interessado %></td>
        <td><%= link_to 'Editar', edit_interessado_path(interessado) %></td>
        <td><%= link_to 'Deletar', interessado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


