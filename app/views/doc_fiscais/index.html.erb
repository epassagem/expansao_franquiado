        <td><%= doc_fiscai.socio_serasa %></td>
        <td><%= doc_fiscai.socio_fpc %></td>
        <td><%= doc_fiscai.socio_bens_imob %></td>
        <td><%= doc_fiscai.socio_imposto_renda_pf %></td>
