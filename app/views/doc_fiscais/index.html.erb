        <td><%= doc_fiscai.socio_comprovante_resid %></td>
        <td><%= doc_fiscai.socio_certidao_casamento %></td>
        <td><%= link_to 'Show', doc_fiscai %></td>
        <td><%= link_to 'Edit', edit_doc_fiscai_path(doc_fiscai) %></td>
        <td><%= link_to 'Destroy', doc_fiscai, method: :delete, data: { confirm: 'Are you sure?' } %></td>
