      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @doc_fiscais.each do |doc_fiscai| %>
      <tr>
        <td><%= doc_fiscai.rg %></td>
        <td><%= doc_fiscai.cpf %></td>
        <td><%= doc_fiscai.serasa %></td>
        <td><%= doc_fiscai.fpc %></td>
        <td><%= doc_fiscai.bens_imob %></td>
        <td><%= doc_fiscai.imposto_renda_pf %></td>
        <td><%= doc_fiscai.imposto_renda_pj %></td>
